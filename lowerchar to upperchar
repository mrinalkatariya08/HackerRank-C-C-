#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
        char lowerChar, upperChar;
      scanf("%c", &lowerChar);
     upperChar= lowerChar-32;
     printf("The uppercase of %c is %c", lowerChar,upperChar);
    return 0;
}
